<div class="syntaxplugin">
	<table cellspacing="0" cellpadding="0" border="0" width="100%">
		<tbody>
			#if ( $codeTitle )
			<tr>
				<td colspan="2" class="syntaxplugin_title">$codeTitle</td>
			</tr>
			#end
			<tr>
			#if( !$codeContainer.hideLineNum )
			<td class="syntaxplugin_gutter">
			#foreach( $codeRow in $codeContainer.codeRows ) #set ( $lineNum = $velocityCount + $codeContainer.getFirstLine() - 1 )
				<div class="line#if( $codeRow.highlighted ) highlighted#end">$lineNum</div>
			#end
			</td>
			#end
			<td class="syntaxplugin_code">
				<div class="syntaxplugin_container">
				#foreach( $codeRow in $codeContainer.codeRows )
					<div class="line#if( $codeRow.highlighted ) highlighted#end">#if($codeRow.code.size() == 0)&nbsp;#end#foreach( $code in $codeRow.code )<code class="$code.style">$code.text</code>#end</div>
				#end
				</div>
			</td>
			</tr>
		</tbody>
	</table>
</div>
