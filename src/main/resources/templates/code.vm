<div class="syntaxhighlighter_jira">
	<table cellspacing="0" cellpadding="0" border="0">
		<tbody>
			<tr>
			#if( !$codeContainer.hideLineNum )
			<td>
			#foreach( $codeRow in $codeContainer.codeRows )
				<div>$velocityCount</div>
			#end
			</td>
			#end
			<td>
			#foreach( $codeRow in $codeContainer.codeRows )
				<div>
					#foreach( $code in $codeRow.code )<code class="syntaxhighlighter_jira $code.style">$code.text</code>#end
				</div>
			#end
			</td>
			</tr>
		</tbody>
	</table>
</div>
