<div id="syntaxplugin" class="syntaxplugin" style="margin: 1em 0em; overflow: scroll; border: 1px dashed #bbb; border-radius: 5px !important;">
<table cellspacing="0" cellpadding="0" border="0" width="100%" style="font-size: 1em; line-height: 1.4em !important; font-weight: normal; font-style: normal; color: black;">
	#if ( $codeTitle || $codeCollapsed )
	<thead>
	#if ( $codeTitle )
		<tr id="syntaxplugin_title">
			<td bgcolor="#f5f5f5" style="font-family: Arial,sans-serif; color: #333; border-bottom: 1px solid #bbb; background-color: #f5f5f5 !important; font-weight: bold; line-height: 1em;" #if( $codeContainer.showLineNums ) colspan="2"#end>
				<p style="margin: 5px 15px; ">$codeTitle</p>
			</td>
		</tr>
	#end
	#if ( $codeCollapsed )
	<tr id="syntaxplugin_show">
		<td #if( $codeContainer.showLineNums ) colspan="2"#end bgcolor="#f5f5f5" style="background-color: #f5f5f5 !important; line-height: 1em;">
			<p style="margin: 5px 15px; color: #3b73af; cursor: pointer;" onclick="var tbody = this.parentNode.parentNode.parentNode.parentNode.getElementsByTagName('tbody'); tbody[tbody.length -1].style.display = 'inline'; this.parentNode.parentNode.style.display = 'none'; return false;">$codeCollapsed</p>
		</td>
	</tr>
	#end
	</thead>
	#end
	<tbody  #if ( $codeCollapsed ) style="display: none;"#end>
		#foreach( $codeRow in $codeContainer.codeRows ) 
		<tr id="syntaxplugin_code_and_gutter">
			#if( $codeContainer.showLineNums )
			<td #if( $codeRow.highlighted ) bgcolor="#6ce26c" #else bgcolor="#f5f5f5" #end style="#if( $codeRow.highlighted ) background-color: #6ce26c !important; #else background-color: #f5f5f5 !important; #end line-height: 1.4em !important; border-right: 1px solid #bbb; text-align: right; width: 2.7em; padding: 0em; vertical-align: top;">
			#set ( $lineNum = $velocityCount + $codeContainer.getFirstLine() - 1 )
				<pre style="font-size: 1em; margin: 0 0.5em 0 1em !important; width: auto;"><span style="#if( $codeRow.highlighted ) color: white; #else color: #707070; #end font-family: 'Consolas', 'Bitstream Vera Sans Mono', 'Courier New', Courier, monospace !important; margin: 0 0.5em 0 1em !important;">$lineNum</span></pre>
			</td>
			#end
			<td #if( $codeRow.highlighted ) bgcolor="#e0e0e0" #end style="#if( $codeRow.highlighted ) background-color: #e0e0e0 !important;#end line-height: 1.4em !important; padding: 0em; vertical-align: top;">
					<pre style="font-size: 1em; margin: 0 1em !important; width: auto;">#if($codeRow.code.size() == 0)&nbsp;#else#foreach( $code in $codeRow.code )<span style="$code.style font-family: 'Consolas', 'Bitstream Vera Sans Mono', 'Courier New', Courier, monospace !important;">$code.text</span>#end#end</pre>
			</td>
		</tr>
		#end
	</tbody>
</table>
</div>
